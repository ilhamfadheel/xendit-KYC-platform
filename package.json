{
  "name": "xendit-kyc-platform",
  "version": "1.0.0",
  "author": "Ilham Fadhil",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilhamfadheel/xendit-kyc-platform.git"
  },
  "description": "Know Your Customer (KYC) Platform for Xendit 2-day Work Trial",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --no-cache --watchAll",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "sequelize",
    "postgres",
    "swagger"
  ],
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.20.0",
    "axios": "^1.7.5",
    "canvas": "^2.11.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "face-api.js": "^0.22.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/pg": "^8.11.8",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
